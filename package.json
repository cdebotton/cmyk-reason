{
  "name": "cmyk-reason",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:reason": "bsb -make-world",
    "prebuild:server": "rm -rf dist/server",
    "build:server": "wait-on lib/js/app/ServerApp.bs.js && tsc",
    "prebuild:webpack": "rm -rf dist/client",
    "build:webpack": "wait-on lib/es6/client/Client.bs.js && NODE_ENV=production webpack",
    "build": "npm-run-all  --parallel 'build:reason' 'build:server' 'build:webpack'",
    "start:reason": "bsb -make-world -w",
    "start:server": "wait-on lib/js/app/ServerApp.bs.js && nodemon server/index.tsx",
    "start:webpack": "wait-on lib/es6/client/Client.bs.js && webpack-serve",
    "start": "NODE_ENV=development npm-run-all --parallel 'start:reason' 'start:server' 'start:webpack'"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/koa": "^2.0.46",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-static": "^4.0.0",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "bs-platform": "^4.0.1",
    "graphql_ppx": "^0.2.6",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.0-rc",
    "wait-on": "^2.1.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-11",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
