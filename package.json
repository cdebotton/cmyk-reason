    "apollo-utilities": "^1.0.17",
    "bcryptjs": "^2.4.3",
    "bs-css": "cdebotton/bs-css#add-min-max-content",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tag": "^2.9.2",
